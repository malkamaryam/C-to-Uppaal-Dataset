<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int a = 48;
int b = 18;
int temp = 0;
</declaration>
	<template>
		<name>GCD</name>
		<location id="id0" x="0" y="0">
			<name x="-17" y="-34">start</name>
		</location>
		<location id="id1" x="180" y="0">
			<name x="161" y="8">loop</name>
		</location>
		<location id="id2" x="360" y="0">
			<name x="348" y="-34">end</name>
		</location>
		<init ref="id0"/>
		<transition id="id3">
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
		<transition id="id4">
			<source ref="id1"/>
			<target ref="id1"/>
			<label kind="guard" x="153" y="-102">b != 0</label>
			<label kind="assignment" x="59" y="-85">temp = b, b = a % b, a = temp</label>
			<nail x="136" y="-59"/>
			<nail x="229" y="-59"/>
		</transition>
		<transition id="id5">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="238" y="0">b == 0</label>
		</transition>
	</template>
	<system>gcd = GCD();
system GCD;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
