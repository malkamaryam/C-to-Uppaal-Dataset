<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>
int i = 0;
int j = 0;
</declaration>
	<template>
		<name>NestedCounter</name>
		<location id="id0" x="0" y="0">
			<name x="-10" y="-30">start</name>
		</location>
		<location id="id1" x="200" y="0">
			<name x="153" y="-34">outer_check</name>
		</location>
		<location id="id2" x="200" y="150">
			<name x="161" y="238">inner_check</name>
		</location>
		<location id="id3" x="400" y="150">
			<name x="374" y="161">inner_end</name>
		</location>
		<location id="id4" x="400" y="0">
			<name x="391" y="-34">end</name>
		</location>
		<init ref="id0"/>
		<transition id="id5">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="60" y="0">i = 0</label>
		</transition>
		<transition id="id6">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="153" y="59">i &lt; 3</label>
			<label kind="assignment" x="153" y="76">j = 0</label>
		</transition>
		<transition id="id7">
			<source ref="id2"/>
			<target ref="id2"/>
			<label kind="guard" x="187" y="195">j &lt; 2</label>
			<label kind="assignment" x="170" y="221">j = j + 1</label>
			<nail x="260" y="220"/>
			<nail x="150" y="220"/>
		</transition>
		<transition id="id8">
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="guard" x="300" y="150">!(j &lt; 2)</label>
		</transition>
		<transition id="id9">
			<source ref="id3"/>
			<target ref="id1"/>
			<label kind="assignment" x="300" y="60">i = i + 1</label>
		</transition>
		<transition id="id10">
			<source ref="id1"/>
			<target ref="id4"/>
			<label kind="guard" x="270" y="-25">!(i &lt; 3)</label>
		</transition>
	</template>
	<system>
nested = NestedCounter();
system NestedCounter;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
