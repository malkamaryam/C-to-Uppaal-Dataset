<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int num = 20;
int div = 5;
int result = -1;
</declaration>
	<template>
		<name>DivisibilityChecker</name>
		<location id="id0" x="-34" y="0">
			<name x="-59" y="-34">start</name>
		</location>
		<location id="id1" x="263" y="-85">
			<name x="229" y="-119">divisible</name>
		</location>
		<location id="id2" x="255" y="102">
			<name x="199" y="120">not_divisible</name>
		</location>
		<location id="id3" x="586" y="8">
			<name x="569" y="-25">done</name>
		</location>
		<init ref="id0"/>
		<transition id="id4">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="guard" x="42" y="-76">num % div == 0</label>
		</transition>
		<transition id="id5">
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="51" y="76">num % div != 0</label>
		</transition>
		<transition id="id6">
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="assignment" x="374" y="-68">result = 1</label>
		</transition>
		<transition id="id7">
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="assignment" x="382" y="68">result = 0</label>
		</transition>
	</template>
	<system>system DivisibilityChecker;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
