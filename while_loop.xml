<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int i = 0;</declaration>
	<template>
		<name>Counter</name>
		<location id="id0" x="0" y="0">
			<name x="-10" y="-30">start</name>
		</location>
		<location id="id1" x="200" y="0">
			<name x="170" y="-30">check</name>
		</location>
		<location id="id2" x="399" y="0">
			<name x="389" y="-30">end</name>
		</location>
		<init ref="id0"/>
		<transition id="id3">
			<source ref="id1"/>
			<target ref="id1"/>
			<label kind="guard" x="170" y="51">i &lt; 5</label>
			<label kind="assignment" x="161" y="85">i = i + 1</label>
			<nail x="238" y="76"/>
			<nail x="153" y="76"/>
		</transition>
		<transition id="id4">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="59" y="0">i = 0</label>
		</transition>
		<transition id="id5">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="272" y="-25">!(i &lt; 5)</label>
		</transition>
	</template>
	<system>counter = Counter();
system Counter;
</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
