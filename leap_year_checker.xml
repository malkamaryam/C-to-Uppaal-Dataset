<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int year = 2022;
int isLeap = 0;
  </declaration>
	<template>
		<name>LeapYearChecker</name>
		<location id="id0" x="50" y="50">
			<name x="25" y="17">start</name>
		</location>
		<location id="id1" x="220" y="50">
			<name x="195" y="59">check</name>
		</location>
		<location id="id2" x="697" y="51">
			<name x="689" y="18">end</name>
		</location>
		<init ref="id0"/>
		<transition id="id3">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="85" y="25">isLeap = 0</label>
		</transition>
		<transition id="id4">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="204" y="0">((year % 4 == 0) &amp;&amp; (year % 100 != 0)) || (year % 400 == 0)</label>
			<label kind="assignment" x="408" y="-17">isLeap = 1</label>
		</transition>
		<transition id="id5">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="195" y="119">!(((year % 4 == 0) &amp;&amp; (year % 100 != 0)) || (year % 400 == 0))</label>
			<label kind="assignment" x="399" y="136">isLeap = 0</label>
			<nail x="450" y="110"/>
		</transition>
	</template>
	<system>leapYearChecker = LeapYearChecker();
system LeapYearChecker;
  </system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
