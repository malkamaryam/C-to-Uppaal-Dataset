<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int num = 12;      // input number
int i;        // loop index
int result;   // last product
  </declaration>
	<template>
		<name>TableOfNumber</name>
		<location id="id0" x="-68" y="0">
			<name x="-78" y="-30">start</name>
		</location>
		<location id="id1" x="200" y="0">
			<name x="161" y="8">procedure</name>
		</location>
		<location id="id2" x="442" y="0">
			<name x="432" y="-30">done</name>
		</location>
		<init ref="id0"/>
		<transition id="id3">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="-8" y="0">i = 1, result = 0</label>
		</transition>
		<transition id="id4">
			<source ref="id1"/>
			<target ref="id1"/>
			<label kind="guard" x="161" y="-85">i &lt;= 10</label>
			<label kind="assignment" x="76" y="-102">result = num * i, i = i + 1</label>
			<nail x="119" y="-68"/>
			<nail x="272" y="-68"/>
		</transition>
		<transition id="id5">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="297" y="-25">i &gt; 10</label>
		</transition>
	</template>
	<system>system TableOfNumber;
  </system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
