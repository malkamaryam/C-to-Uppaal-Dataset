<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int arr[5] = {3,5,7,2,4};
int Sum = 0;
int i = 0;
</declaration>
	<template>
		<name>ArraySum</name>
		<location id="id0" x="0" y="0">
			<name x="-10" y="-30">Start</name>
		</location>
		<location id="id1" x="200" y="0">
			<name x="178" y="8">Check</name>
		</location>
		<location id="id2" x="195" y="-119">
			<name x="185" y="-149">Update</name>
		</location>
		<location id="id3" x="510" y="0">
			<name x="500" y="-30">Done</name>
		</location>
		<init ref="id0"/>
		<transition id="id4">
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
		<transition id="id5">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="144" y="-68">i &lt; 5</label>
		</transition>
		<transition id="id6">
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="assignment" x="238" y="-93">Sum = Sum + arr[i], i++</label>
			<nail x="246" y="-68"/>
		</transition>
		<transition id="id7">
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="guard" x="331" y="-25">i &gt;= 5</label>
		</transition>
	</template>
	<system>arraySum = ArraySum(); system ArraySum;</system>
	<queries>
		<query>
			<formula/>
			<comment/>
		</query>
	</queries>
</nta>
