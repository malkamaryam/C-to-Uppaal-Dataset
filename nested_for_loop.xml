<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.6//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_6.dtd'>
<nta>
	<declaration>int Sum = 0;
int i = 0;
int j = 0;</declaration>
	<template>
		<name>NestedLoop</name>
		<location id="id0" x="0" y="0">
			<name x="-17" y="-34">start</name>
		</location>
		<location id="id1" x="200" y="0">
			<name x="165" y="-30">check_i</name>
		</location>
		<location id="id2" x="289" y="153">
			<name x="306" y="144">check_j</name>
		</location>
		<location id="id3" x="493" y="0">
			<name x="476" y="-34">end</name>
		</location>
		<init ref="id0"/>
		<transition id="id4">
			<source ref="id2"/>
			<target ref="id2"/>
			<label kind="guard" x="272" y="229">j &lt; 5</label>
			<label kind="assignment" x="170" y="263">Sum = Sum + (i + j), j = j + 1</label>
			<nail x="391" y="255"/>
			<nail x="212" y="255"/>
		</transition>
		<transition id="id5">
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="40" y="0">i = 0</label>
		</transition>
		<transition id="id6">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="161" y="76">i &lt; 5</label>
			<label kind="assignment" x="178" y="93">j = 0</label>
			<nail x="178" y="51"/>
		</transition>
		<transition id="id7">
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="guard" x="331" y="-25">!(i &lt; 5)</label>
		</transition>
		<transition id="id8">
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="238" y="42">!(j &lt; 5)</label>
			<label kind="assignment" x="246" y="59">i = i + 1</label>
		</transition>
	</template>
	<system>nested = NestedLoop();
system nested;
</system>
	<queries>
		<query>
			<formula>A[] not deadlock</formula>
			<comment>System runs correctly</comment>
		</query>
	</queries>
</nta>
